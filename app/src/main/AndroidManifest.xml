<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="Mobile.gavelgo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity"></activity>
        <activity android:name=".Activity.UserLogin"></activity>
        <activity android:name=".Activity.SignupActivity"></activity>
        <activity android:name=".Activity.ResetPassordActivity"></activity>
        <activity android:name=".Activity.AddPartnerInformation"></activity>
        <activity android:name=".Activity.AddProductActivity"></activity>
        <activity android:name=".Activity.AddConsumerInformation"></activity>


    </application>

</manifest>